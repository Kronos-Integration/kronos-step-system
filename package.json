{
  "name": "kronos-step-system",
  "version": "0.0.0-semantic-release",
  "description": "step to execute system commands",
  "keywords": [
    "kronos-step"
  ],
  "main": "system.js",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "node ./node_modules/.bin/jsdoc lib/*.js",
    "test": "node ./node_modules/.bin/mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-step-system.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-step-system/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-step-system#readme",
  "dependencies": {
    "kronos-endpoint": "^2.13.1",
    "kronos-step": "^5.3.4"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.1.7",
    "istanbul": "0.4.4",
    "jsdoc": "3.4.0",
    "kronos-service-manager": "^3.4.14",
    "kronos-test-step": "3.1.4",
    "mocha": "3.0.2",
    "semantic-release": "6.3.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
